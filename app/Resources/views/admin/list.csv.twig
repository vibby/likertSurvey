Email,Key,URL,Email feedback,feedbackTeam,Inséré le,Nb collab,collab,source,domain,is manager,{% for key in questionKeys %}{{ key }},{% endfor %}

{% for respondent in respondents %}
    {{- respondent.email -}},
    {{- respondent.key -}},
    {{- absolute_url(path('homepage', {'key': respondent.key})) -}},
    {{- respondent.emailFeedback -}},
    {{- respondent.feedbackTeam -}},
    {{- respondent.createdDate|date('Y-m-d') -}},
    {{- respondent.subordinates|length -}},
    {%- for subordinate in respondent.subordinates -%}
        {%- if not loop.first -%}|{%- endif -%}
        {{- subordinate.email -}}
    {%- endfor -%},
    {{- respondent.source -}},
    {{- respondent.domain -}},
    {{- respondent.isManager -}},
    {%- for key in questionKeys -%}
        {%- set keyFound = false -%}
        {%- for keyA,answer in respondent.response -%}
            {%- if keyA == key -%}
                {{ answer }},
                {%- set keyFound = true -%}
            {%- endif -%}
        {%- endfor -%}
        {%- if not keyFound -%}
            ,
        {%- endif -%}
    {%- endfor %}

{% endfor %}
